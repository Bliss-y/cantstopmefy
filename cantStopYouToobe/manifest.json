{
  "manifest_version": 3,
  "name": "CantStopYouToo-be",
  "description": "youtube toolkit :)",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ],
  "host_permissions": [
    "http://localhost:8001/*"
  ],
  "content_scripts": [
    {
      "css": [
        "index.css"
      ],
      "js": [
        "scripts/index.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}